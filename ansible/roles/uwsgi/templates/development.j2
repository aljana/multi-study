[uwsgi]
plugin          = python
module          = _project.wsgi:application
wsgi-file       = {{ root_dir }}/_project/wsgi.py
chdir           = {{ root_dir }}/_project
virtualenv      = {{ home_dir }}/.virtualenv/{{ virtualenv }}
master          = true
processes       = 6
enable-threads  = true
cpi-affinity    = 1
listen          = 65534
no-orphans      = true
vacuum          = true
thunder-lock    = true
py-autoreload   = true
